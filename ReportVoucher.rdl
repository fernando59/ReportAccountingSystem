    <DataSet Name="DetailVoucher">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@idVoucher">
            <Value>=Parameters!idVoucher.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select tv.gloss,tv.amountOwed,tv.amountOwedAlt,tv.amountAssets,tv.amountAssetsAlt,concat(ta.codeAccount,'-',ta.nameAccount) as nameAccount from tblVoucherDetail tv,tblAccounts ta
where idVoucher = @idVoucher and tv.idAccount = ta.idAccount</CommandText>
      </Query>
