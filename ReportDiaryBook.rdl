  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@idCompany">
            <Value>=Parameters!idCompany.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@idPeriod">
            <Value>=Parameters!idPeriod.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT tv.dateVoucher,tvd.idVoucherDetail,concat(ta.codeAccount,'-',ta.nameAccount) as nameAccount,tvd.gloss,tvd.amountOwed,tvd.amountAssets,tvd.amountOwedAlt,tvd.amountAssetsAlt
FROM tblVoucher tv,tblVoucherDetail tvd,tblAccounts ta,tblCompanies tcom, tblManagements tm,tblPeriods tp
where tv.idVoucher = tvd.idVoucher and tvd.idAccount = ta.idAccount  and tv.idCompany = tcom.idCompany and tm.idCompany = tcom.idCompany and tp.idManagement = tm.idManagement 
and tv.idCompany =@idCompany and tv.dateVoucher &gt; tp.startDate and tv.dateVoucher &lt;tp.endDate and tp.idPeriod in (@idPeriod)</CommandText>
      </Query>
      <Fields>
        <Field Name="dateVoucher">
          <DataField>dateVoucher</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="idVoucherDetail">
          <DataField>idVoucherDetail</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="nameAccount">
