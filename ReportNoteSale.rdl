  <DataSets>
    <DataSet Name="Details">
      <Query>
        <DataSourceName>Source</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@idNote">
            <Value>=Parameters!idNote.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select tl.idArticle,tl.idNote,tl.nroLote,tl.quantityDetail,tl.priceSale,ta.nameArticle,(tl.quantityDetail*tl.priceSale) as subTotal
from tblDetail tl, tblArticles ta
where tl.idArticle = ta.idArticle and tl.idNote =@idNote</CommandText>
      </Query>
