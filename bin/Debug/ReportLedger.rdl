    <DataSet Name="GetTable">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@idCompany">
            <Value>=Parameters!idCompany.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@idManagement">
            <Value>=Parameters!idManagement.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@idPeriod">
            <Value>=Parameters!idPeriod.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select ta.nameAccount,tv.idVoucher,tv.gloss,tv.typeVoucher,tvd.amountOwed,tvd.amountAssets,tv.dateVoucher,tvd.amountOwedAlt,tvd.amountAssetsAlt,(tvd.amountOwed-tvd.amountAssets) as balance
from tblVoucher tv,tblVoucherDetail tvd, tblAccounts ta,tblCompanies tco,tblManagements tm,tblPeriods tp
where tv.idVoucher = tvd.idVoucher and tco.idCompany = tv.idCompany 
and tvd.idAccount = ta.idAccount and tco.idCompany = tm.idCompany 
and tm.idManagement = tp.idManagement and tv.dateVoucher &gt;= tp.startDate and tv.dateVoucher &lt;=tp.endDate and  tv.idCompany =@idCompany and tm.idManagement =@idManagement and tp.idPeriod in (@idPeriod)
</CommandText>
      </Query>
      <Fields>
        <Field Name="nameAccount">
          <DataField>nameAccount</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="idVoucher">
