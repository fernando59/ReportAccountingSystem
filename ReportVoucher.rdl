  <DataSets>
    <DataSet Name="Voucher">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@idVoucher">
            <Value>=Parameters!idVoucher.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select tv.serieVoucher,tv.tc,tv.typeVoucher,tv.statusVoucher,tc.nameCompany,tv.dateVoucher,tv.gloss,tcu.nameCurrency,tc.idCompany
from tblVoucher  tv,tblCompanies tc,tblCurrency tcu
where tv.idVoucher = @idVoucher and tc.idCompany =tv.idCompany and tcu.idCurrency = tv.idCurrency </CommandText>
      </Query>
