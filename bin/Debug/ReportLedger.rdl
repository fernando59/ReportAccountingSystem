    <DataSet Name="getCurrency">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@idCompany">
            <Value>=Parameters!idCompany.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select tc.idCurrency,tc.nameCurrency,tcc.exchange
from tblCompanyCurrency tcc,tblCurrency tc
where tcc.active = 1 and 
(tcc.idCurrencyMain = tc.idCurrency or tcc.idCurrencyAltern = tc.idCurrency) and tcc.idCompany = @idCompany</CommandText>
      </Query>
      <Fields>
        <Field Name="idCurrency">
