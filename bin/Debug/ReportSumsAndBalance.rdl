  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@idCompany">
            <Value>=Parameters!idCompany.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@idManagement">
            <Value>=Parameters!idManagement.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select concat(ta.codeAccount,'-',ta.nameAccount) as nameAccount,sum(tvd.amountOwed) as debe ,sum(tvd.amountAssets) as haber,sum(tvd.amountOwedAlt) as debeAlt,sum(tvd.amountAssetsAlt) as haberAlt,
case
	when (sum(tvd.amountOwed)-sum(tvd.amountAssets)&gt;=0) then cast((sum(tvd.amountOwed)-sum(tvd.amountAssets)) as decimal(6,2))
	else '0'
	end as debe2,
	case
	when (sum(tvd.amountAssets)-sum(tvd.amountOwed)&gt;=0) then cast((sum(tvd.amountAssets)-sum(tvd.amountOwed)) as decimal(6,2))
	else '0'
	end as haber2,
	case
	when (sum(tvd.amountOwedAlt)-sum(tvd.amountAssetsAlt)&gt;=0) then cast((sum(tvd.amountOwedAlt)-sum(tvd.amountAssetsAlt)) as decimal(6,2))
	else '0'
	end as debeAlt2,
		case
	when (sum(tvd.amountAssetsAlt)-sum(tvd.amountOwedAlt)&gt;=0) then cast((sum(tvd.amountAssetsAlt)-sum(tvd.amountOwedAlt)) as decimal(6,2))
	else '0'
	 end as haberAlt2
from tblAccounts ta,tblVoucherDetail tvd,tblVoucher tv,tblCompanies tc,tblManagements tm
where ta.idAccount= tvd.idAccount and ta.idCompany=@idCompany and tv.idVoucher = tvd.idVoucher and tc.idCompany = tv.idCompany and tm.idCompany = tc.idCompany and tm.idManagement = @idManagement
GROUP BY ta.nameAccount,ta.codeAccount</CommandText>
      </Query>
      <Fields>
        <Field Name="nameAccount">
          <DataField>nameAccount</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
