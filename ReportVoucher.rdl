            <Value>=Parameters!idCompany.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText> select top 1 tc.nameCurrency 
 from tblCompanyCurrency tcc,tblCurrency tc
where tcc.idCompany =@idCompany and tcc.idCurrencyAltern = 1 and  tcc.idCurrencyMain =  tc.idCurrency </CommandText>
      </Query>
      <Fields>
        <Field Name="nameCurrency">
