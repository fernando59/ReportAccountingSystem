  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@idCompany">
            <Value>=Parameters!idCompany.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@idManagement">
            <Value>=Parameters!idManagement.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        tblPeriods.namePeriod, tblPeriods.startDate, tblPeriods.endDate, tblPeriods.state, tblManagements.nameManagement, tblCompanies.nameCompany
FROM            tblPeriods INNER JOIN
                         tblManagements ON tblPeriods.idManagement = tblManagements.idManagement INNER JOIN
                         tblCompanies ON tblManagements.idCompany = tblCompanies.idCompany
WHERE        (tblCompanies.idCompany = @idCompany) AND (tblManagements.idManagement = @idManagement)</CommandText>
      </Query>
