  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@idCompany">
            <Value>=Parameters!idCompany.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT   CONCAT(tblAccounts.codeAccount,'  ',tblAccounts.nameAccount) as name,tblAccounts.codeAccount, tblCompanies.nameCompany,tblAccounts.levelAccount
FROM            tblAccounts INNER JOIN
                         tblCompanies ON tblAccounts.idCompany = tblCompanies.idCompany
WHERE      tblCompanies.idCompany = @idCompany
order by tblAccounts.codeAccount asc </CommandText>
      </Query>
      <Fields>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="codeAccount">
          <DataField>codeAccount</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="nameCompany">
          <DataField>nameCompany</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="levelAccount">
          <DataField>levelAccount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
